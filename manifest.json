{
  "manifest_version": 2,
  "name": "CraftyWars",
  "short_name": "CW",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [
        "js/background.js"
      ]
    }
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "web_accessible_resources": [
    "bootstrap/fonts/*"
  ]
}
